source :rubygems

gem "rails", "3.2.11"

group :development do
   gem "pry", :require => false
   gem "awesome_print", :require => false
   gem "pry-rails"
end

group :development, :test do
   gem "rspec-rails"
   gem "capybara"
   gem "sqlite3"
end

group :test do
   gem "factory_girl"
   gem "factory_girl-preload"
end